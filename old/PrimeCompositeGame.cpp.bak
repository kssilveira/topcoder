using namespace std;

const int MAX = 500000;

bitset<MAX> isnotprime;
bitset<MAX> gone, solvegone;
bitset<MAX> memowincomposite;
int memosolvecomposite[MAX];
vector<int> primes;
 
class PrimeCompositeGame {
	public:
  PrimeCompositeGame() {
    primes.push_back(2);
    isnotprime[0] = isnotprime[1] = 1;
    for (int i = 3; i < MAX; i += 2) {
      if (!isnotprime[i]) {
        primes.push_back(i);
        for (int j = i + i; j < MAX; j += i) {
          isnotprime[j] = 1;
        }
      }
    }
  }

  int solvecomposite(int N, int K) {
    int& res = memosolvecomposite[N];
    if (solvegone[N]) return res;
    solvegone[N] = true;
    if (wincomposite(N, K)) {
      res = INT_MAX;
      for (int i = K; i >= 1; i--) {
        int next = N - i;
        if (next > 2 && ((next % 2 == 0) || isnotprime[next])) {
          if (!winprime(next, K)) {
            res = min(res, solveprime(next, K));
          }
        }
      }
      if (res == INT_MAX || res == INT_MIN) return res = 0;
      return ++res;
    } else {
      res = INT_MIN;
      for (int i = K; i >= 1; i--) {
        int next = N - i;
        if (next > 2 && ((next % 2 == 0) || isnotprime[next])) {
          //if (!winprime(next, K)) {
            res = max(res, solveprime(next, K));;
          //}
        }
      }
      if (res == INT_MAX || res == INT_MIN) return res = 0;
      return ++res;
    }
  }

  bool wincomposite(int N, int K) {
    if (gone[N]) return memowincomposite[N];
    gone[N] = true;
    bool res = true;
    for (int i = K; i >= 1; i--) {
      int next = N - i;
      if (next > 2 && ((next % 2 == 0) || isnotprime[next])) {
        res = res && winprime(next, K);
        if (!res) break;
      }
    }
    return memowincomposite[N] = !res;
  }

  bool winprime(int N, int K) {
    bool res = true;
    for (int i = K; i >= 1; i--) {
      int next = N - i;
      if (next > 1 && !isnotprime[next] && (next % 2 || next == 2)) {
        res = res && wincomposite(next, K);
        if (!res) break;
      }
    }
    return !res;
  }

  int solveprime(int N, int K) {
    if (winprime(N, K)) {
      int res = INT_MAX;
      for (int i = K; i >= 1; i--) {
        int next = N - i;
        if (next > 1 && !isnotprime[next] && (next % 2 || next == 2)) {
          if (!wincomposite(next, K)) {
            res = min(res, solvecomposite(next, K));
            break;
          }
        }
      }
      if (res == INT_MAX || res == INT_MIN) return res = 0;
      return ++res;
    } else {
      int res = INT_MIN;
      for (int i = K; i >= 1; i--) {
        int next = N - i;
        if (next > 1 && !isnotprime[next] && (next % 2 || next == 2)) {
          //if (wincomposite(next, K)) {
            res = max(res, solvecomposite(next, K));
          //}
        }
      }
      if (res == INT_MAX || res == INT_MIN) return res = 0;
      return ++res;
    }
  }

	int theOutcome(int N, int K) {
    gone.reset();
    solvegone.reset();
    //cout << s(primes) << endl;
    //r(i, 10) {
      //cout << primes[i] << ", ";
    //}

    //r(i, 30) {
      //cout << i << ' ' << winprime(i, K) << endl;
    //}

		winprime(N, K);
    int res = solveprime(N, K);
    if (!winprime(N, K)) res = -res;
		return res;
	}
	
